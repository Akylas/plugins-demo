{
    "description": "Nativescript Plugins Demo App",
    "license": "MIT",
    "readme": "NativeScript Application",
    "author": "Martin Guillon<martin.guillon@akylas.fr>",
    "main": "app/main",
    "scripts": {
        "prepare": "ts-patch install -s",
        "sentry-set-commits": "sentry-cli releases set-commits --auto --ignore-missing",
        "run.android.production": "cross-var devns run android --release --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.sentry=0 --env.sourceMap=0 --env.noconsole=0  --config=nativescript.config.logging.js",
        "run.android.production.sentry": "cross-var devns run android --release --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc_sentry --env.sentryUpload=0 --env.sourceMap=0 --config=nativescript.config.sentrylogging.js  --env.devlog",
        "run.android.timeline": "cross-var devns run android --config=nativescript.config.timeline --release --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.timeline",
        "build.android.production": "cross-var devns build android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.sentry=0 --env.sourceMap=0  --env.report --aab",
        "build.android.production.fdroid": "cross-var devns build android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.report --gradleArgs=-PsplitEnabled  --gradleArgs=-PabiFilters=arm64-v8a,armeabi-v7a",
        "build.android.production.fdroid.sentry": "npm run build.android.production.fdroid -- --config=nativescript.config.sentry.js --env.adhoc_sentry"
    },
    "dependencies": {
        "@akylas/nativescript": "8.4.1",
        "@nativescript-community/l": "4.2.26",
        "@nativescript-community/motion-activity": "1.1.0",
        "@nativescript-community/perms": "2.2.20",
        "@nativescript-community/sentry": "4.6.2",
        "@nativescript-community/systemui": "1.1.13",
        "@nativescript-community/text": "1.5.10",
        "@nativescript-community/ui-canvas": "4.4.5",
        "@nativescript-community/ui-collectionview": "4.0.64",
        "@nativescript-community/ui-label": "1.2.16",
        "dayjs": "1.11.6",
        "helpful-decorators": "2.1.0",
        "nativescript-vue": "2.9.3",
        "vue-property-decorator": "~9.1.2",
        "vue-string-filter": "2.1.0"
    },
    "devDependencies": {
        "@akylas/fontmin": "^0.9.9",
        "@babel/core": "7.20.5",
        "@nativescript/android": "8.4.0",
        "@nativescript/core": "8.4.1",
        "@nativescript/ios": "8.4.0",
        "@nativescript/types-android": "8.4.0",
        "@nativescript/types-ios": "8.4.0",
        "@nativescript/webpack": "5.0.12",
        "@sentry/cli": "2.10.0",
        "@sentry/webpack-plugin": "1.20.0",
        "@typescript-eslint/eslint-plugin": "5.45.0",
        "@typescript-eslint/parser": "5.45.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "babel-loader": "^9.1.0",
        "cross-var": "1.1.0",
        "eslint": "8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.8.0",
        "merge-options": "^3.0.4",
        "merge-properties-files": "^1.1.6",
        "nativescript-set-version": "0.3.1",
        "nativescript-vue-template-compiler": "2.9.3",
        "prettier": "^2.8.0",
        "rimraf": "3.0.2",
        "sass": "^1.56.1",
        "sass-loader": "^13.2.0",
        "scss-symbols-parser": "^2.0.1",
        "string-replace-loader": "^3.1.0",
        "typescript": "~4.8.4",
        "vue": "2.6.14",
        "vue-class-component": "7.2.6",
        "vue-i18n": "9.2.2",
        "vue-loader": "15.9.8"
    }
}
